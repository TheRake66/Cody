{
    "console": {
        "enable_history": true,
        "print_logo": true,
        "max_width": 80,
        "max_height": 25,
        "auto_update": true,
        "php_default_ip": "localhost",
        "php_default_port": 6600
    },
    "security": {
        "redirect_to_https": false,
        "self_xss_prevent": false,
        "cookie_path": "/",
        "cookie_domain": "www.{PROJECT_NAME}.com",
        "cookie_only_https": false,
        "cookie_prevent_xss": false,
        "cookie_lifetime": 0
    },
    "render": {
        "use_minifying": false,
        "catch_error": true, 
        "show_error_message": true,
        "show_supervisor": true,
        "debug_border_component": true,
        "wait_dom_loaded": true,
        "delay_after_load": 200,
        "api_beautify_json": true
    },
    "log": {
        "use_log_file": true,
        "ip_identify": false,
        "throw_if_unwritable": true,
        "max_lenght": 1000,
        "query": {
            "print_query": true,
            "print_parameters": true,
            "print_results": true,
            "print_mail": true,
            "print_mail_header": true,
            "print_mail_content": true
        }
    },
    "website_head": {
        "meta_charset": "utf-8",
        "meta_description": "{PROJECT_NAME}",
        "meta_keywords": "{PROJECT_NAME}",
        "meta_author": "{PROJECT_AUTHOR}",
        "meta_viewport": "width=device-width, initial-scale=1.0",
        "meta_theme_color": "#3B78FF",
        "robot_robots": "index, follow",
        "equiv_cache_control": "no-cache",
        "equiv_pragma": "no-cache",
        "equiv_cache": "no-cache",
        "equiv_expires": -1,
        "tag_title": "{PROJECT_NAME}",
        "link_favicon": "favicon.ico"
    },
    "region": {
        "timezone": "Europe/Paris",
        "locale": "fr_FR",
        "main_lang": "fr"
    },
    "session": {
        "open_session": true,
        "multiple_session": true,
        "session_name": "{PROJECT_NAME}",
        "token_lifetime_days": 31,
        "regenerate_delay": -1,
        "regenerate_delete_old": false
    },
    "database": {
        "default_database": "{PROJECT_NAME}",
        "progressive_connection": true,
        "databases_list": [
            {
				"dsn_login": "root", 
				"dsn_password": "", 
				"dsn_host": "localhost", 
				"dsn_port": "3306", 
				"dsn_name": "{PROJECT_NAME}", 
                "dsn_encoding": "utf8", 
                "dsn_type": "mysql",
                "throw_sql_error": true,
                "throw_transaction": true,
                "emulate_prepare": false,
                "persistent_mode": false
            }
        ]
    }
}